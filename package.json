{
  "name": "@kreed/ridelines-chainring",
  "version": "1.0.0",
  "description": "backend API for ridelines.xyz - map for all your intervals.icu activities",
  "main": "dist/index.mjs",
  "types": "src/types.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kreed/ridelines-chainring.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "tsx watch -r dotenv/config scripts/dev-server.ts",
    "postinstall": "openapi-typescript extern/intervals-openapi.json -o src/generated/intervals-types.ts || echo ''",
    "build": "tsdown",
    "test": "OPENSSL_CONF=./openssl_legacy.cnf vitest --run",
    "test:watch": "OPENSSL_CONF=./openssl_legacy.cnf vitest",
    "type-check": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome check --fix .",
    "check": "npm run type-check && npm run lint"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.30.0",
    "@aws-lambda-powertools/metrics": "^2.30.0",
    "@aws-lambda-powertools/tracer": "^2.30.0",
    "@aws-sdk/client-dynamodb": "^3.962.0",
    "@aws-sdk/client-sqs": "^3.962.0",
    "@aws-sdk/cloudfront-signer": "^3.957.0",
    "@aws-sdk/lib-dynamodb": "^3.962.0",
    "@clerk/backend": "^2.29.0",
    "@middy/core": "^6.4.5",
    "@trpc/server": "^11",
    "openapi-fetch": "^0.15.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@tsconfig/node22": "^22.0.5",
    "@types/aws-lambda": "^8.10.159",
    "@types/node": "^22.0.0",
    "dotenv": "^17.2.3",
    "openapi-typescript": "^7.10.1",
    "tsdown": "^0.18.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "ridelines",
    "trpc",
    "lambda",
    "api"
  ],
  "author": "Chris Eby",
  "license": "MIT"
}
